<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../../common/xsd/model.xsd" label="Machine">
    <fields>
        <field name="id" type="long" label="编号" allow-empty="false"/>
        <field name="label" type="string" label="Label" />
        <field name="status" type="enum:status" label="状态"/>
        <field name="createTime" type="date" label="Create Time"/>
        <field name="mahjong" type="mahjong" label="麻将商户" />        
    </fields>
	<enums>
        <enum name="status" label="状态" editable="false">
            <item name="normal" label="正常"/>
            <item name="loan" label="出借中"/>
            <item name="inactive" label="停用"/>
        </enum>
    </enums>
</model>
